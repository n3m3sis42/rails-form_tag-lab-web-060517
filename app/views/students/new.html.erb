<h3>Student Form</h3>
<%= form_tag students_path do %>
  <p><%= label_tag :'student[first_name]', "First Name: " %><%= text_field_tag :'student[first_name]', nil, id: "first_name" %>
  <p><%= label_tag :'student[last_name]', "Last Name: " %><%= text_field_tag :'student[last_name]', nil, id: "last_name" %></p>

  <%= submit_tag "Submit Student "%>
<% end %>
